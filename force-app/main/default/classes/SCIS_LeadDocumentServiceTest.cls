@isTest
public class SCIS_LeadDocumentServiceTest {
@isTest
    static void testAddDocumentToLead() {
        // Test data setup
        Lead testLead = new Lead(
            FirstName = 'Adams',
            LastName = 'Hebrew',
            Company = 'Test Company',
            Status = 'Accepted'
            //Lead_ID__c = '00QQz0000054tHZMAY'
            // Add any other required fields here
        );
        insert testLead;

        String leadId = testLead.id;
        String documentName = 'Test Document';
        String base64Data = EncodingUtil.base64Encode(Blob.valueOf('image/png;base64,')); // Convert a string to base64
        
        RestRequest request = new RestRequest();
        RestResponse response = new RestResponse();
        
        // Set request parameters
        request.requestURI = '/services/apexrest/leadDocument';
        request.addParameter('leadId', leadId);
        request.addParameter('documentName', documentName);
        request.addParameter('base64Data', base64Data);
        request.httpMethod = 'POST';
        
        Test.startTest();
        // Call the REST service method
        RestContext.request = request;
        RestContext.response = response;
        SCIS_LeadDocumentService.addDocumentToLead(leadId, documentName, base64Data);
        Test.stopTest();
        
        // Assertions
        // Check if the document was successfully added
        Attachment[] attachments = [SELECT Id, Name FROM Attachment WHERE ParentId = :leadId];
        System.assertNotEquals(0, attachments.size(), 'Attachment should have been added');
        System.assertEquals(documentName, attachments[0].Name, 'Attachment name should match');
        // Add more assertions if needed
    }
}