public class ImprovementElmhurstAPI {
    public class AssessmentRequest {
        public String inputXmlBase64 { get; set; }
        public String rdsapGuid { get; set; }
        public List<Recommendation> recommendations { get; set; }
        public String calculationType { get; set; }
        public String integratorReference { get; set; }
    }

    // Recommendation Object
    public class Recommendation {
        public String item { get; set; }
        public String name { get; set; }
        public Boolean selected { get; set; }
        public Integer sapRating { get; set; }
        public Integer eiRating { get; set; }
        public Decimal capitalCost { get; set; }
        public Decimal fuelCostSaving { get; set; }
        public Decimal paybackPeriod { get; set; }
        public Decimal coEmissionsSaving { get; set; }
        public Decimal carbonCostEffectiveness { get; set; }
    }

    // Method to send the request
    public static String calculateAssessment() {
        // Create the request body
        AssessmentRequest request = new AssessmentRequest();
        request.inputXmlBase64 = '';
        request.rdsapGuid = '867AD43F-8C3A-49D7-B9DB-789B8286DDD4';
        request.calculationType = 'Cumulative';
        request.integratorReference = '000003';

        // Adding a recommendation object
        request.recommendations = new List<Recommendation>();
        Recommendation rec = new Recommendation();
        rec.item = 'B4';
        rec.name = 'Party wall insulation';
        rec.selected = true;
        rec.sapRating = 77;
        rec.eiRating = 84;
        rec.capitalCost = 0;
        rec.fuelCostSaving = 0;
        rec.paybackPeriod = 0;
        rec.coEmissionsSaving = 0;
        rec.carbonCostEffectiveness = 0;
        request.recommendations.add(rec);

        String requestBody = JSON.serialize(request);

        // Create the HTTP request
        HttpRequest req = new HttpRequest();
        req.setEndpoint('https://elmhurst-api-staging.elmhurstenergy.net/api/Rdsap/Results/Survey?api-version=2.0');
        req.setMethod('POST');
        
        // Set the request body
        req.setBody(requestBody);

        System.debug('Request Body: ' + req.getBody());

        Http http = new Http();
        try {
            HttpResponse res = http.send(req);
            return res.getBody();
        } catch (Exception e) {
            System.debug('Error: ' + e.getMessage());
            return 'Error: ' + e.getMessage();
        }
    }
}